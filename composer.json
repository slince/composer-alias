{
    "name": "slince/composer-alias",
    "description": "Composer Alias Plugin",
    "keywords": ["composer", "composer-plugin", "alias", "composer-alias", "composer-alias-plugin"],
    "type": "composer-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^1.0",
        "phpunit/phpunit": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Slince\\ComposerAlias\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slince\\ComposerAlias\\Test\\": "tests/"
        }
    },
    "extra": {
        "class": "Slince\\ComposerAlias\\ComposerAlias"
    }
}
